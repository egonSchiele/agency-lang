{
  "name": "agency-lang",
  "version": "0.0.67",
  "description": "The Agency language",
  "main": "lib/index.js",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:agency": "node ./dist/scripts/agency.js test tests/agency",
    "build": "rm -rf dist/ && tsc && tsc-alias && cp -r lib/agents dist/lib/agents",
    "start": "node dist/index.js",
    "templates": "typestache ./lib/templates",
    "typecheck": "tsc --noEmit",
    "agency": "node ./dist/scripts/agency.js",
    "compile": "node ./dist/scripts/agency.js compile",
    "ast": "node ./dist/scripts/agency.js ast",
    "graph": "node ./dist/scripts/agency.js graph",
    "fmt": "node ./dist/scripts/agency.js fmt",
    "eval": "node ./dist/scripts/agency.js eval",
    "fixtures": "node ./dist/scripts/agency.js fixtures",
    "rr": "node ./dist/scripts/agency.js rr",
    "diagnostics": "node ./dist/scripts/agency.js diagnostics",
    "preprocess": "node ./dist/scripts/agency.js preprocess",
    "circular-deps": "npx dpdm --circular lib/parser.ts",
    "postinstall": "node scripts/hooks/postinstall.js"
  },
  "bin": {
    "agency": "./dist/scripts/agency.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egonSchiele/agency-lang.git"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.js"
    }
  },
  "type": "module",
  "types": "./dist/lib/index.d.ts",
  "keywords": [],
  "author": "Aditya Bhargava",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/egonSchiele/agency-lang/issues"
  },
  "homepage": "https://github.com/egonSchiele/agency-lang#readme",
  "dependencies": {
    "beautiful-mermaid": "^0.1.3",
    "commander": "^14.0.3",
    "egonlog": "^0.0.2",
    "prompts": "^2.4.2",
    "smoltalk": "^0.0.18",
    "tarsec": "^0.1.8",
    "typestache": "^0.4.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/prompts": "^2.4.9",
    "cli-highlight": "^2.1.11",
    "nanoid": "^5.1.6",
    "termcolors": "github:egonSchiele/termcolors",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
