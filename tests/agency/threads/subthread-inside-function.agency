def nextTwo(): number[] {
  subMsgs = subthread {
    foo: number[] = llm("What are the next 2 prime numbers after that?")
  }
  return { 
    foo: foo,
    subMsgs: subMsgs
  }
}

node main() {
  msgs = thread {
    res1: number[] = llm("What are the first 5 prime numbers?")
    res2 = nextTwo()
    res3 = nextTwo()
  }
  return { 
    res1: res1,
    res2: res2,
    res3: res3,
    msgs: msgs
  }
}